{
  "name": "com.relirk.portfolio",
  "displayName": "Portfolio",
  "version": "1.0.0",
  "homepage": ".",
  "private": true,
  "license": "Apache-2.0",
  "author": "Francisco Olvera",
  "description": "Meus projetos com build em device.",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:cordova": "rm -rf ./build ./www && yarn build && node __script.js && cp -av ./build ./www",
    "set:cordova": "yarn build:cordova && mv build www",
    "cordova:browser": "yarn set:cordova && yarn run:browser",
    "cordova:android": "yarn set:cordova && yarn run:android",
    "cordova:ios": "yarn set:cordova && yarn run:ios",
    "build:all": "cordova build",
    "build:browser": "cordova build browser --verbose",
    "build:android": "cordova build android --verbose",
    "build:android:prod": "cordova build android --release -- --keystore='../android.keystore' --storePassword=android --alias=mykey",
    "build:ios": "cordova build ios --verbose",
    "run:browser": "yarn build:browser && cordova run browser",
    "run:android": "yarn build:android && cordova run android",
    "run:ios": "yarn build:ios && cordova run ios"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^5.1.1",
    "material-ui-popup-state": "^1.5.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-toastify": "^5.5.0",
    "recharts": "^2.0.0-beta.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-plugin-root-import": "^6.4.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "customize-cra": "^0.4.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "prettier": "^1.18.2",
    "react-app-rewired": "^2.1.3"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "browser",
      "android",
      "ios"
    ]
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
